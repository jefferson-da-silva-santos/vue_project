{
  "name": "vue_project",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "concurrently \"npm run watch:sass\" \"vite\"",
    "build": "vue-tsc -b && vite build",
    "preview": "vite preview",
    "tauri": "tauri",
    "watch:sass": "sass src/assets/sass/main.scss src/assets/css/styles.css -w",
    "compile:sass": "sass src/assets/sass/main.scss src/assets/css/styles.comp.css",
    "prefix:css": "postcss src/assets/css/styles.comp.css --use autoprefixer --output src/assets/css/styles.prefix.css",
    "compress:css": "postcss src/assets/css/styles.prefix.css --use cssnano --output src/assets/css/styles.css",
    "build:css": "npm-run-all compile:sass prefix:css compress:css"
  },
  "dependencies": {
    "@primeuix/themes": "^2.0.3",
    "@primevue/themes": "^4.5.4",
    "boxicons": "^2.1.4",
    "chart.js": "^4.5.1",
    "npm-run-all": "^4.1.5",
    "primevue": "^4.5.4",
    "vee-validate": "^4.15.1",
    "vue": "^3.5.24",
    "vue-router": "^5.0.2",
    "yup": "^1.7.1"
  },
  "devDependencies": {
    "@tauri-apps/cli": "^2.10.0",
    "@types/node": "^24.10.1",
    "@vitejs/plugin-vue": "^6.0.1",
    "@vue/tsconfig": "^0.8.1",
    "autoprefixer": "^10.4.20",
    "concurrently": "^9.2.1",
    "cssnano": "^7.0.6",
    "postcss": "^8.4.47",
    "postcss-cli": "^11.0.0",
    "sass": "^1.80.4",
    "typescript": "~5.9.3",
    "vite": "^7.2.4",
    "vue-tsc": "^3.1.4"
  }
}
